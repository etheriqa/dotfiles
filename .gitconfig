[include]
	path = .gitconfig.local
[user]
	name = Kaede Takamori
	email = etheriqa@gmail.com
[color]
	ui = auto
[credential]
	helper = osxkeychain
[core]
	excludesfile = /Users/etheriqa/.gitignore
	editor = vim
[github]
	user = etheriqa
[alias]
	a = add
	ap = add --patch
	au = add --update
	b = branch
	bb = bisect bad
	bd = branch -d
	bg = bisect good
	bl = blame
	bm = branch -m
	br = bisect reset
	bs = bisect start
	c = commit --verbose
	ca = commit --verbose --all
	cam = commit --verbose --all --amend
	cm = commit --verbose --amend
	co = checkout
	cob = checkout -b
	coo = checkout --ours
	cot = checkout --theirs
	cp = cherry-pick
	d = diff
	f = fetch
	fa = fetch --all
	g = grep
	m = merge --no-ff
	ma = merge --abort
	mc = merge --continue
	pl = pull --rebase
	ps = push
	r = reset
	rb = rebase
	rba = rebase --abort
	rbc = rebase --continue
	rbi = rebase --interactive
	rbs = rebase --skip
	rv = revert
	s = status
	sm = submodule
	smu = submodule update
	stc = stash clear
	stl = stash list
	stp = stash pop
	sts = stash save
