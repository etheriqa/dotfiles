# vim:noet:ts=8:sw=8:sts=8:
[include]
	path = ~/.gitconfig.local
[user]
	name = Kaede Takamori
	email = etheriqa@gmail.com
[color]
	ui = auto
[core]
	excludesfile = ~/.gitignore
	editor = vim
[github]
	user = etheriqa
[alias]
	a = add
	ap = add --patch
	au = add --update
	b = branch
	bb = bisect bad
	bd = branch --delete
	bD = branch -D
	bg = bisect good
	bl = blame -w
	bm = branch --move
	br = bisect reset
	bs = bisect start
	c = commit --verbose
	ca = commit --verbose --all
	cam = commit --verbose --all --amend
	cm = commit --verbose --amend
	cma = commit --verbose --amend --all
	co = checkout
	cob = checkout -b
	coo = checkout --ours
	cot = checkout --theirs
	d = diff
	dc = diff --cached
	f = fetch
	fa = fetch --all
	fo = fetch origin
	g = grep
	m = merge --no-ff
	ma = merge --abort
	mc = merge --continue
	mf = merge --ff-only
	ms = merge --squash
	p = cherry-pick
	pl = pull
	ps = push
	ra = remote add
	rb = rebase
	rba = rebase --abort
	rbc = rebase --continue
	rbi = rebase --interactive
	rbs = rebase --skip
	rh = reset --hard
	rr = remote rm
	rs = reset --soft
	rv = revert
	s = status --short --branch
	sc = stash clear
	sl = stash list
	sm = submodule
	smu = submodule update
	sp = stash pop
	ss = stash save
[push]
	default = current
[pull]
	rebase = true
