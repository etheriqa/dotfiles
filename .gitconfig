[include]
	path = .gitconfig.local
[user]
	name = Kaede Takamori
	email = etheriqa@gmail.com
[color]
	ui = auto
[credential]
	helper = osxkeychain
[core]
	excludesfile = /Users/etheriqa/.gitignore
	editor = vim
[github]
	user = etheriqa
[alias]
	a = add
	b = branch
	bl = blame
	bs = bisect
	c = commit -v
	co = checkout
	cp = cherry-pick
	d = diff
	f = fetch
	g = grep
	m = merge --no-ff
	pl = pull --rebase
	ps = push
	r = reset
	rb = rebase
	rv = revert
	s = status
	sm = submodule
	st = stash
